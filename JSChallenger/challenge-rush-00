// Write a function that takes a string as argument 
// Create an object that has a property with key 'key' and a value equal to the string 
// Return the object 

function myFunction (a){
    let key = a 
    return {key} 
}

console.log(myFunction("pepe")) //Expected: pepe
console.log(myFunction("john")) //Expected: john
console.log(myFunction(42)) //Expected: 42
